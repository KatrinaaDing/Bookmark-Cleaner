{
   "name" : "Bookmark Data Cleaner",
   "version" : "2.0",
   "manifest_version" : 2,
   "description" : "Removing browsing data when deleting a bookmark.",
   "permissions": [ "bookmarks", "history", "browsingData", "cookies"],
   "browser_action" : {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [{
      "js": ["page.js"]
    }]
}