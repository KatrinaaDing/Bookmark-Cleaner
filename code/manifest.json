{
   "name" : "Bookmark Data Cleaner",
   "version" : "2.2",
   "manifest_version" : 3,
   "description" : "Removing browsing data when deleting a bookmark.",
   "permissions": [ "bookmarks", "history", "browsingData" ],
   "action" : {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   }
}