{
   "name" : "Bookmark Cleaner",
   "version" : "2.0",
   "manifest_version" : 2,
   "description" : "Removing browsing data when deleting a bookmark.",
   "permissions": [ "bookmarks", "history", "browsingData", "cookies"],
   "browser_action" : {
      "default_icon": "icon.png",
      "default_popup": "page.html"
   },
   "content_scripts": [{
      "js": ["jquery.min.js", "page.js"],
      "matches": ["http://*/*", "https://*/*"]
    }]
}